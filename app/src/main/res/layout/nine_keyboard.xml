<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">


    <EditText
        android:id="@+id/filter_edit"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/colorTransparency"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textCursorDrawable="@drawable/edittext_cursor_shap"
        android:textSize="@dimen/px46" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px1"
        android:layout_marginTop="@dimen/px14"
        android:background="@drawable/nine_keyboard_line_edit" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px45"
        android:shrinkColumns="0,1,2"
        android:stretchColumns="0,1,2">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/button30"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:background="@drawable/ninekey_board_selector"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/px42"
                    android:layout_height="@dimen/px44"
                    android:background="@drawable/nine_keyboard_button_clear" />
            </LinearLayout>

            <include
                android:id="@+id/button31"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />

            <LinearLayout
                android:id="@+id/button32"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14"
                android:background="@drawable/ninekey_board_selector"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/px51"
                    android:layout_height="@dimen/px32"
                    android:src="@drawable/nine_keyboard_button_back" />

            </LinearLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px4">

            <include
                android:id="@+id/button00"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106" />

            <include
                android:id="@+id/button01"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />

            <include
                android:id="@+id/button02"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px4">

            <include
                android:id="@+id/button10"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106" />

            <include
                android:id="@+id/button11"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />

            <include
                android:id="@+id/button12"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px4">

            <include
                android:id="@+id/button20"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106" />

            <include
                android:id="@+id/button21"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />

            <include
                android:id="@+id/button22"
                layout="@layout/nine_keyboard_button"
                android:layout_width="@dimen/px176"
                android:layout_height="@dimen/px106"
                android:layout_marginLeft="@dimen/px14" />
        </TableRow>

    </TableLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px4"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/button40"
            android:layout_width="@dimen/px256"
            android:layout_height="@dimen/px126"
            android:background="@drawable/nine_keyboard_dial_selector"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center">

            <ImageView
                android:layout_width="@dimen/px70"
                android:layout_height="@dimen/px38"
                android:background="@drawable/nine_keyboard_video_button" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/button41"
            android:layout_width="@dimen/px256"
            android:layout_height="@dimen/px126"
            android:layout_marginLeft="@dimen/px14"
            android:background="@drawable/nine_keyboard_dial_selector"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center">

            <ImageView
                android:layout_width="@dimen/px53"
                android:layout_height="@dimen/px55"
                android:background="@drawable/nine_keyboard_voice_button" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>